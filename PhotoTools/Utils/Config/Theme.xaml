<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="CRgbM1">#FF3A3A41</Color>
    <Color x:Key="CRgbM2">#FF8F8FA3</Color>
    <Color x:Key="CRgbM3">#FFFFFAFF</Color>
    <Color x:Key="CRgbB1">#FF329BA8</Color>
    <Color x:Key="CRgbB2">#FFD33C4A</Color>
    <Color x:Key="CRgbAll">#FF555555</Color>
    <Color x:Key="CRgbOk">#FFA1E510</Color>
    <Color x:Key="CRgbOkBis">#FF2DC8AB</Color>
    <Color x:Key="CRgbNok">#FFD33C4A</Color>
    <Color x:Key="CRgbNokBis">#FFDC562E</Color>
    
    <SolidColorBrush x:Key="RgbM1" Color="{StaticResource CRgbM1}"/>
    <SolidColorBrush x:Key="RgbM2" Color="{StaticResource CRgbM2}"/>
    <SolidColorBrush x:Key="RgbM3" Color="{StaticResource CRgbM3}"/>
    <SolidColorBrush x:Key="RgbB1" Color="{StaticResource CRgbB1}"/>
    <SolidColorBrush x:Key="RgbB2" Color="{StaticResource CRgbB2}"/>
    <SolidColorBrush x:Key="RgbAll" Color="{StaticResource CRgbAll}"/>
    <SolidColorBrush x:Key="RgbOk" Color="{StaticResource CRgbOk}"/>
    <SolidColorBrush x:Key="RgbOkBis" Color="{StaticResource CRgbOkBis}"/>
    <SolidColorBrush x:Key="RgbNok" Color="{StaticResource CRgbNok}"/>
    <SolidColorBrush x:Key="RgbNokBis" Color="{StaticResource CRgbNokBis}"/>
    
</ResourceDictionary>